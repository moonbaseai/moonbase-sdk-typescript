{
  "dxt_version": "0.1",
  "name": "@moonbaseai/mcp",
  "version": "0.1.0-alpha.7",
  "description": "The official MCP Server for the Moonbase API",
  "author": {
    "name": "Moonbase",
    "email": "support@moonbase.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonbaseai/moonbase-sdk-typescript.git"
  },
  "homepage": "https://github.com/moonbaseai/moonbase-sdk-typescript/tree/main/packages/mcp-server#readme",
  "server": {
    "type": "node",
    "entry_point": "${__dirname}/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "MOONBASE_API_KEY": "${user_config.MOONBASE_API_KEY}"
      }
    }
  },
  "user_config": {
    "MOONBASE_API_KEY": {
      "title": "api_key",
      "description": "Your Moonbase API key.",
      "required": true,
      "type": "string"
    }
  },
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
